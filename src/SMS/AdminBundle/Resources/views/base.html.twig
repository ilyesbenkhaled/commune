		<script src="{{asset('bundles/smsadmin/vendor/morrisjs/morris.min.js')}}"></script>
		<script src="{{asset('bundles/smsadmin/data/morris-data.js')}}"></script>

		<!-- Custom Theme JavaScript -->
		<script src="{{asset('bundles/smsadmin/dist/js/sb-admin-2.js')}}"></script>
